{
	"name": "vscode-scriptable",
	"publisher": "gebeto",
	"displayName": "Scriptable",
	"description": "Scriptable App Integration extension",
	"version": "0.0.10",
	"repository": {
		"type": "git",
		"url": "https://github.com/gebeto/scriptable-vscode"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:scriptable.importScript",
		"onCommand:scriptable.initialize"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scriptable.initialize",
				"title": "Scriptable: Initialize sources"
			},
			{
				"command": "scriptable.importScript",
				"title": "Scriptable: Import Script"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"vpub": "yarn version --patch && vsce package && vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.7",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vsce": "^1.88.0",
		"vscode-test": "^1.5.0"
	}
}
