{
	"name": "vscode-scriptable",
	"publisher": "gebeto",
	"displayName": "Scriptable",
	"description": "Scriptable App Integration extension",
	"version": "0.0.14",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gebeto/scriptable-vscode"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:scriptable.importScript",
		"onCommand:scriptable.initialize"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scriptable.initialize",
				"title": "Scriptable: Initialize sources"
			},
			{
				"command": "scriptable.importScript",
				"title": "Scriptable: Import Script"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"build": "vsce package",
		"publish": "vsce publish",
		"vpub": "yarn version --patch && yarn build && yarn publish"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^17.0.9",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.7.0",
		"mocha": "^9.1.4",
		"typescript": "^4.1.3",
		"vsce": "^2.6.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"glob": "^7.2.0"
	}
}
